{
    "name": "teqneers/ext-direct-bundle",
    "type": "library",
    "description": "A Symfony 2 bundle to integrate Sencha Ext JS Ext.direct into a Symfony 2 application",
    "homepage": "https://github.com/teqneers/ext-direct-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Gehrig",
            "email": "gehrig@teqneers.de",
            "homepage": "http://opensource.teqneers.de/"
        }
    ],
    "autoload": {
        "psr-4": {
            "TQ\\Bundle\\ExtDirectBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TQ\\Bundle\\ExtDirectBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/http-kernel": "~2.8|~3.0",
        "symfony/config": "~2.8|~3.0",
        "symfony/dependency-injection": "~2.8|~3.0",
        "symfony/routing": "~2.8|~3.0",
        "teqneers/ext-direct": "~1.0,>=1.0.5",
        "twig/twig": "~1.24",
        "jms/serializer-bundle": "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8,!=4.8.20|~5.1",
        "symfony/framework-bundle": "~2.8|~3.0",
        "symfony/security-bundle": "~2.8|~3.0",
        "symfony/templating": "~2.8|~3.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/teqneers/ext-direct.git"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
