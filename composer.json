{
    "name": "teqneers/ext-direct-bundle",
    "version": "0.1.x-dev",
    "type": "library",
    "description": "A Symfony 2 bundle to integrate Sencha Ext JS Ext.direct into a Symfony 2 application",
    "homepage": "https://github.com/teqneers/ext-direct-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Gehrig",
            "email": "gehrig@teqneers.de",
            "homepage": "http://opensource.teqneers.de/"
        }
    ],
    "autoload": {
        "psr-4": {
            "TQ\\Bundle\\ExtDirectBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TQ\\Bundle\\ExtDirectBundle\\Tests\\": "tests/Tests/"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "symfony/http-kernel": "~2.6|~3.0.0",
        "symfony/config": "~2.6|~3.0.0",
        "symfony/dependency-injection": "~2.6|~3.0.0",
        "symfony/routing": "~2.6|~3.0.0",
        "teqneers/ext-direct": "dev-master",
        "twig/twig": "~1.18",
        "jms/serializer-bundle": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7"
    }
}
